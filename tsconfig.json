{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "outDir": "./dist",

        /* Strict Type-Checking Options */
        "strict": true,
        "typeRoots": ["./node_modules/@types", "./src/types"],
        // "types": [],
        "esModuleInterop": true,

        /* Advanced Options */
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    }
}
